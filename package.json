{
  "name": "little-latte-lane",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:notifications": "node scripts/test-notifications.js",
    "health": "node scripts/health-check.js",
    "clean": "rm -rf .next dist coverage test-results playwright-report",
    "analyze": "madge --circular --extensions ts,tsx src/",
    "build:analyze": "cross-env ANALYZE=true next build",
    "export": "next build && next export",
    "db:generate-types": "supabase gen types --lang=typescript --project-id=$NEXT_PUBLIC_SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "precommit": "npm run typecheck && npm run lint && npm run format:check",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "docker:test": "powershell -ExecutionPolicy Bypass -File ./docker/test-connection.ps1",
    "docker:test-bash": "bash ./docker/test-connection.sh",
    "db:backup": "docker exec little-latte-postgres pg_dump -U postgres little_latte_lane > backup.sql",
    "dev:docker": "npm run docker:up && npm run dev"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^10.5.0",
    "@sentry/tracing": "^7.120.4",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/qrcode": "^1.5.5",
    "@types/react-color": "^3.0.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.525.0",
    "next": "15.4.2",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-color": "^2.19.3",
    "react-colorful": "^5.6.1",
    "react-contenteditable": "^3.3.7",
    "react-datepicker": "^8.4.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.5.2",
    "recharts": "^3.1.0",
    "resend": "^4.7.0",
    "sonner": "^2.0.7",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.14",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/postcss": "^4",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "eslint-plugin-import": "^2.31.0",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "supabase": "^1.215.2",
    "tailwindcss": "^3.4.17",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5"
  }
}
